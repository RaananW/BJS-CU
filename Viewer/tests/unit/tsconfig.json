{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "forceConsistentCasingInFileNames": true,
        "noResolve": false,
        "experimentalDecorators": true,
        "declaration": false,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": false,
        "sourceMap": true,
        "lib": [
            "dom",
            "es2015.promise",
            "es5"
        ],
    },
    "include": [
        "./src/**/*.ts"
    ]
}